<emulation>
  <emuname>RealTrace</emuname>
  <emuType>TIME</emuType>
  <emuresourceType>MEM</emuresourceType>
  <!--date format: 2014-10-10T10:10:10 -->
  <emustartTime>2014-10-10T10:10:10</emustartTime>
  <!--duration in seconds -->
  <emustopTime>60</emustopTime>
  
  <distributions >
     <name>realTrace</name>
     <startTime>0</startTime>
     <!--duration in seconds -->
     <distribution href="/distributions/real_trace" name="real_trace" />
     <trace>/home/melo/crunchbang_2013-10-22-13-27</trace>
     <emulator href="/emulators/lookbusy" name="lookbusy" />
     <emulator-params>
        <resourceType>MEM</resourceType>
	    <!--time between iterations in usec (default 1000)-->
		<malloclimit>4004</malloclimit>
		<memSleep>0</memSleep>
     </emulator-params>
  </distributions>

  <log>
  	<!-- Use value "1" to enable logging(by default logging is off)  -->
  	<enable>0</enable>
  	<!-- Use seconds for setting probe intervals(if logging is enabled default is 3sec)  -->
  	<frequency>3</frequency>
  	<logLevel>debug</logLevel>
  </log>

</emulation>
