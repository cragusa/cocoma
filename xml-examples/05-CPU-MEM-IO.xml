<emulation>
  <name>Emu-CPU-RAM-IO</name>
  <emulationType>Mix</emulationType>
  <resourceType>Mix</resourceType>
  <startTime>2013-11-25T18:44:00</startTime>
  <!--duration in seconds -->
  <stopTime>40</stopTime>
  <log>1</log>
  
  <distributions>
     <name>CPU-dis-1</name>
     <startTime>0</startTime>
     <!--duration in seconds -->
     <duration>15</duration>
     <granularity>3</granularity>
     <distribution href="/distributions/linear" name="linear" />
	<!--cpu utilization distribution range-->
      <startLoad>10</startLoad>
      <stopLoad>90</stopLoad>
      <emulator href="/emulators/lookbusy" name="lookbusy" />
      <emulator-params>
        <!--more parameters will be added -->
        <resourceType>CPU</resourceType>
	<!--Number of CPUs to keep busy (default: autodetected)-->
	<ncpus>0</ncpus>
      </emulator-params>
  </distributions>

  <distributions >
	<name>MEM-dis-2</name>
     <startTime>5</startTime>
     <!--duration in seconds -->
     <duration>15</duration>
     <granularity>3</granularity>
     <distribution href="/distributions/linear" name="linear" />
     <!--Megabytes for memory -->
      <startLoad>500</startLoad>
      <stopLoad>1000</stopLoad>
      <emulator href="/emulators/lookbusy" name="lookbusy" />
      <emulator-params>
        <resourceType>MEM</resourceType>
	<!--time between iterations in usec (default 1000)-->
	<memSleep>0</memSleep>
      </emulator-params>
  </distributions>

  <distributions >
	<name>IO-dis-3</name>
     <startTime>10</startTime>
     <!--duration in seconds -->
     <duration>20</duration>
     <granularity>4</granularity>
     <distribution href="/distributions/linear" name="linear" />
     <!--Megabytes for memory -->
      <startLoad>50</startLoad>
      <stopLoad>200</stopLoad>
      <emulator href="/emulators/lookbusy" name="lookbusy" />
      <emulator-params>
        <resourceType>IO</resourceType>
	<!--time between iterations in usec (default 1000)-->
	<ioSleep>0</ioSleep>
	<ioBlockSize>100</ioBlockSize>
      </emulator-params>
  </distributions>
  
</emulation>
